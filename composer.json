{
    "name": "celltrak/event-queue-bundle",
    "type": "symfony-bundle",
    "description": "Distributed event queue bundle for Symfony2",
    "keywords": ["event queue", "message queue", "symfony"],
    "authors": [
        {
            "name": "Mike Turoff",
            "email": "mturoff@celltrak.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "^2.1",
        "celltrak/CTLib": "^2.5 || dev-develop",
        "celltrak/redis-bundle": "^1.1"
    },
    "autoload": {
        "psr-4": { "Celltrak\\EventQueueBundle\\": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:celltrakdev/ct20_ctlib.git"
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:celltrakdev/celltrakredisbundle.git"
        }
    ]
}
